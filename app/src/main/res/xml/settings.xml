<PreferenceScreen xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <SwitchPreferenceCompat
        app:icon="@mipmap/ic_launcher"
        app:key="key1"
        app:title="@string/sync_title"
        tools:visibility="invisible" />

    <SwitchPreferenceCompat
        app:key="key2"
        app:title="@string/sync_title"
        app:iconSpaceReserved="false"
        />

    <SwitchPreferenceCompat
        app:key="key3"
        app:title="@string/sync_title"

        />


    <SwitchPreference
        app:iconSpaceReserved="false"
        android:title="audio_settings"
        android:key="audio_settings"
        android:summary="audio_settings auto open "
        />

    <PreferenceCategory
        android:layout="@layout/preference_divider"
        android:key="auto_download_settings_divider">

    </PreferenceCategory>

    <SwitchPreference
        app:iconSpaceReserved="false"
        android:title="screen_settings"
        android:key="screen_settings" />

    <PreferenceCategory
        android:layout="@layout/preference_divider"
        android:key="screen_settings_divider">

    </PreferenceCategory>

    <Preference
        app:iconSpaceReserved="false"
        android:title="cancel_screen_setting"
        android:key="cancel_screen_setting" />

    <PreferenceCategory
        android:layout="@layout/preference_category_divider"
        android:key="screen_setting_category">

    </PreferenceCategory>

    <PreferenceCategory
        android:layout="@layout/preference_category_divider"
        android:key="reply_manage_divider">

    </PreferenceCategory>

    <Preference
        app:iconSpaceReserved="false"
        android:title="reply_manage"
        android:key="reply_manage"
        android:widgetLayout="@layout/preference_widget_arrow" />

</PreferenceScreen>